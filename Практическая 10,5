import 'package:flutter/foundation.dart';
import 'package:flutter/material.dart';

void main() {
  runApp(MaterialApp(
      home:  Scaffold(
          body: TextField(
              onSubmitted: (text) {
                if (kDebugMode) {
                  print("onSubmitted");
                }
                if (kDebugMode) {
                  print("Введенный текст: $text");
                }
              },
              onChanged: (text) {
                if (kDebugMode) {
                  print("onChanged");
                }
                if (kDebugMode) {
                  print("Введенный текст: $text");
                }
              }),
          appBar: AppBar(title: Text("METANIT.COM")))
  ));
}
